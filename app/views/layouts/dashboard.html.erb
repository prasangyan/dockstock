<% require "aws/s3" %>
<!doctype html>
<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if lt IE 7 ]> <html lang="en" class="no-js ie6"> <![endif]-->
<!--[if IE 7 ]>    <html lang="en" class="no-js ie7"> <![endif]-->
<!--[if IE 8 ]>    <html lang="en" class="no-js ie8"> <![endif]-->
<!--[if IE 9 ]>    <html lang="en" class="no-js ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--><html lang="en" class="no-js"> <!--<![endif]-->
<head>
  <%= render :file => "layouts/header" %>
  <title>Dashboard</title>
  <!-- CSS : implied media="all" -->
  <%= stylesheet_link_tag "style" %>
  <%= stylesheet_link_tag "jquery.fancybox-1.3.4" %>
  <!-- Uncomment if you are specifically targeting less enabled mobile browsers
  <link rel="stylesheet" media="handheld" href="css/handheld.css?v=2">  -->
  <!-- All JavaScript at the bottom, except for Modernizr which enables HTML5 elements & feature detects -->
  <%= javascript_include_tag "libs/modernizr-1.6.min" %>
</head>
<body>
    <div class="header">
      <%= image_tag "logo.png" %>
      <ul>
        <li><a href="#"  class="selected">Dashboard</a></li>
        <li><%= link_to "Clients", :controller => "clients" %></li>
      </ul>
    </div>
	<%= yield %>
    <%= render :file => "layouts/googleanalytics" %>
    <%= javascript_include_tag "libs/jquery-1.4.2.min" %>
    <%= javascript_include_tag "jquery.mousewheel-3.0.4.pack" %>
    <%= javascript_include_tag "jquery.fancybox-1.3.4.pack" %>
    <%= javascript_include_tag "dashboard" %>
</body>
</html>