<div class="container">
    <div class="content">
        <div class="project-list">
            <h3>History</h3>
            <% unless @history.nil? %>
            <ul>
                    <% @history.each  do |hist| %>
                    <li>
                        <ul class="sub-items">
                            <li><%= hist.description %></li>
                        </ul>
                    </li>
                    <% end %>
                </ul>
                <% else %>
                    <ul>
                      <li>Invalid parameter passed.</li>
                    </ul>
                <% end %>
		    </div>
        <div class="clearfix"></div>
    </div>
    <div class="create_project_box_container"></div>
    <div class="create_project_box"></div>
</div>
